        return $this->actors;
    }

    /**
     * Set producers
     *
     * @param string $producers
     * @return AssetVideoGallery
     */
    public function setProducers($producers)
    {
        $this->producers = $producers;

        return $this;
    }

    /**
     * Get producers
     *
     * @return string 
     */
    public function getProducers()
